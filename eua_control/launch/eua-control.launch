<launch>
  <node name="eua_control" pkg="eua_control" type="controller" output="screen">
    <rosparam>
      <!-- port: "/dev/ttyUSB0" -->
      port: "/dev/ttyACM0"
      baud_rate: 1000000
      joint_device_mapping: {"roll": 1, "pitch": 2, "yaw1": 4, "yaw2": 3} <!-- maps joint names to device IDs -->
    </rosparam>
  </node>
</launch>

