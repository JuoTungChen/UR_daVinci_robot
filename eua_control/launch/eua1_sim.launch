<launch>
  <node name="eua1_controller_sim" pkg="eua_control" type="eua1_controller_sim" output="screen">
    <rosparam>
      joint_device_mapping: {"roll": 1, "wrist": 2, "jaw1": 4, "jaw2": 3} <!-- maps joint names to device IDs -->
      publish_servo_states: true
    </rosparam>
  </node>
</launch>

