<?xml version="1.0"?>
<launch>

  <include file="$(find ur_robot_driver)/launch/ur_control.launch">
    <arg name="robot_ip" value="ursim.lan" />
    <arg name="reverse_port" value="50002" />
    <!-- <arg name="tf_prefix" default="" doc="tf_prefix used for the robot."/> -->
    <arg name="controllers" default="joint_state_controller joint_group_pos_controller"/>
    <!-- <arg name="stopped_controllers" default="joint_group_vel_controller"/> -->
    <arg name="controller_config_file" default="$(find ursurg_config)/config/ur5e_controllers.yaml" />
    <arg name="kinematics_config" value="$(find ursurg_config)/calibration/ursim-5.7.0-ur5e.yaml" />
    <arg name="headless_mode" value="true" />
    <arg name="use_tool_communication" value="false" />
  </include>

</launch>

